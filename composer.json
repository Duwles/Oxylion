{
    "name": "oxylion/oxylion",
    "type": "library",
    "description": "the framework php",
    "keywords": [ "microframework" ],
    "homepage": "https://github.com/duwles/oxylion#readme",
    "license": "MIT",
    "authors": [
        {
            "name": "Bartosz Zwski",
            "email": "duwless@outlook.com"
        }
    ],
    "support": { "issues": "https://github.com/duwles/oxylion/issues" },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-PDO": "*",
        "ext-phar": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-yaml": "*",
        "psr/container": "1.0.0",
        "psr/event-dispatcher": "1.0.0",
        "psr/log": "1.1.3",
        "symfony/yaml": "v5.1.7",
        "symfony/var-dumper": "v5.1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
        "league/flysystem-cached-adapter": "^1.0",
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench-core": "^6.0",
        "pda/pheanstalk": "^4.0"
    },
    "scripts": {
        "oxylion-src": [
            "foreman -v"
        ],
        "post-create-project-cmd": "@oxylion-src"
    },  
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.4.5"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "conflict": {
        "symfony/var-dumper": "<5.0"
    },
    "suggest": {
        "symfony/var-dumper": "For var and obj content preaty browse."
    },
    "autoload": {
        "files": [
            "src/Oxylion/Foundation/Support/helpers.php"
        ],
        "psr-4": {
            "Oxylion\\Component\\": "src/Oxylion/Component/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Oxylion\\": "tests/Oxylion/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}